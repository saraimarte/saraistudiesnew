---
import ClassHomePage from '@layouts/ClassHomePage.astro';
import type { CollectionEntry } from 'astro:content';
import { getCollection } from 'astro:content';

// Fetch all blog posts
const allPosts: CollectionEntry<'fmanage'>[] = (await getCollection('foundations'));


export async function getStaticPaths() {
  const allPosts: CollectionEntry<'foundations'>[] = await getCollection('foundations');

  return allPosts.map(entry => ({
    params: {
      slug: entry.slug
    }
  }));
}

const allArticles = allPosts;
const folderPath1 = "../toolbox";
const folderPath2 = "../rankings";
const folderPath3 = "../ratings";
const pathHome =  './';

---

<ClassHomePage
  folderPath1= {folderPath1} 
  folderPath2 = {folderPath2} 
  folderPath3 = {folderPath3} 
  pathHome = {pathHome} 
  pageTitle="Foundations for Your Career Journey Directory" 
  allArticles = {allArticles} 
  className = "foundations" 
  fullClassTitle = "Foundations for Your Career Journey 33:011:301">
</ClassHomePage>
