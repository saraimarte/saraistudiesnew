---
import ClassHomePage from '@layouts/ClassHomePage.astro';
import type { CollectionEntry } from 'astro:content';
import { getCollection } from 'astro:content';

// Fetch all blog posts
const allCalc2Posts: CollectionEntry<'computerArchitecture'>[] = (await getCollection('computerArchitecture'));


export async function getStaticPaths() {
  const allCalc2Posts: CollectionEntry<'computerArchitecture'>[] = await getCollection('computerArchitecture');

  return allCalc2Posts.map(entry => ({
    params: {
      slug: entry.slug
    }
  }));
}

const allArticles = allCalc2Posts;
const folderPath1 = "../toolbox";
const folderPath2 = "../rankings";
const folderPath3 = "../ratings";
const pathHome =  './';

---

<ClassHomePage 
 folderPath1= {folderPath1} 
  folderPath2 = {folderPath2} 
  folderPath3 = {folderPath3} 
  pathHome = {pathHome}
  pageTitle="Computer Architecture Directory" 
  allArticles = {allArticles} 
  className = "computerArchitecture" 
  fullClassTitle = "Computer Architecture 01:198:211">
</ClassHomePage>
